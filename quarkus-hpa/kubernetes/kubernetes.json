              "scheme" : "HTTP"
            },
            "initialDelaySeconds" : 5,
            "periodSeconds" : 10,
            "successThreshold" : 1,
            "timeoutSeconds" : 10
          },
          "resources" : {
            "limits" : {
              "cpu" : "100m",
              "memory" : "256Mi"
            },
            "requests" : {
              "cpu" : "10m",
              "memory" : "128Mi"
            }
          },
          "startupProbe" : {
            "failureThreshold" : 3,
            "httpGet" : {
              "path" : "/q/health/started",
