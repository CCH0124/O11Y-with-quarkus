            "failureThreshold" : 3,
            "httpGet" : {
              "path" : "/q/health/live",
              "port" : 8080,
              "scheme" : "HTTP"
            },
            "initialDelaySeconds" : 5,
            "periodSeconds" : 10,
            "successThreshold" : 1,
            "timeoutSeconds" : 10
          },
          "name" : "quarkus-hpa",
          "ports" : [ {
            "containerPort" : 8080,
            "name" : "http",
            "protocol" : "TCP"
          } ],
          "readinessProbe" : {
            "failureThreshold" : 3,
            "httpGet" : {
